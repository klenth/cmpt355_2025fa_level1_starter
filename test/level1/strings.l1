/* strings.l1: operations on string values and string variables
[[BEGIN TEST CASES]]
Output: <<5
599
5-100
599-100
-10099-100>>
[[END TEST CASES]]
*/

string p = "5";
string q = "99";
string r = "-100";

print(p);           // 5
print(p + q);       // 599
print(p + r);       // 5-100
print(p + q + r);   // 599-100
print(r + q + r);   //-10099-100
