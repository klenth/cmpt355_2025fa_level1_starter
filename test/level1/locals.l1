/* locals.l1: local variables
[[BEGIN TEST CASES]]
Output: <<-15 1234.5 compiler
-51 5.4321 relipmoc
0 0.0 zero>>
[[END TEST CASES]]
*/

void f() {
    int i = -15;
    double x = 1234.5;
    string s = "compiler";
    g(i, x, s);

    i = -51;
    x = 5.4321;
    s = "relipmoc";
    g(i, x, s);
}

void g(int i, double x, string s) {
    print(i, " ", x, " ", s);
}

f();
g(0, 0, "zero");
