// Precedence: * over +
print(4.0 + 5.0 * 9.0);         // 49.0
print(4.0 + (5.0 * 9.0));       // 49.0
print((4.0 + 5.0) * 9.0);       // 81.0

print(4.0 * 5.0 + 9.0);         // 29.0
print((4.0 * 5.0) + 9.0);       // 29.0
print(4.0 * (5.0 + 9.0));       // 56.0

// Equal precedence of + and - //
print(4.0 - 5.0 + 9.0);         // 8.0
print(4.0 + 5.0 - 9.0);         // 0.0

// Equal precedence of *, /, and %
print(4.0 / 5.0 * 9.0);         // 7.2
print(4.0 * 5.0 / 9.0);         // 2.2222222222222223
print(4.0 * 5.0 % 9.0);         // 2.0
print(4.0 % 5.0 * 9.0);         // 36.0
print(4.0 / 5.0 % 9.0);         // 0.8
print(4.0 % 5.0 / 9.0);         // 0.4444444444444444

// Precedence of (unary) - over (binary) + and -
print(-4.0 + 5.0);              // 1.0
print(-4.0 - 5.0);              // -9.0.0
print(-(4.0 + 5.0));            // -9.0.0
print(-(4.0 - 5.0));            // 1.0

// Just for fun :D
print(------------1.0);         // 1.0
print(-------------1.0);        // -1.0
print(4.0----5.0----9.0);       // 18.0

// Left-associativity of -, /, and %
print(4.0 - 5.0 - 9.0);         // -10.0
print(4.0 / 5.0 / 9.0);         // 0.08888888888888889
print(4.0 % 9.0 % 5.0);         // 4.0.0
